{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"]
      },
      "test": {
        "outputs": ["coverage/**"],
        "dependsOn": []
      },
      "lint": {
        "dependsOn": ["^build"]
      },
      "typecheck": {
        "dependsOn": ["^build"]
      },
      "dev": {
        "dependsOn": ["^build"],
        "cache": false,
        "persistent": true
      },
      "clean": {
        "cache": false
      }
    }
  }